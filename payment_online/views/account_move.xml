<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="account_move_inherit_payment_online" model="ir.ui.view">
    <field name="name">account.move.inherit.payment.online</field>
    <field name="model">account.move</field>
    <field name="inherit_id" ref="account.view_move_form"/>
    <field name="arch" type="xml">

        <xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='other_info']/group[1]/group[@name='sale_info_group']/field[@name='partner_bank_id']" position="after">
            <field name="pay_online"/>
        </xpath>
        
    </field>
  </record>
</odoo>
