<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="account_move_inherit_installed_services" model="ir.ui.view">
    <field name="name">account.move.inherit.installed.services</field>
    <field name="model">account.move</field>
    <field name="inherit_id" ref="account.view_move_form"/>
    <field name="arch" type="xml">

      <xpath expr="//form[1]/sheet[1]/notebook[1]/page[1]/field[@name='invoice_line_ids']/tree[1]/field[@name='product_id']" position="before">
        <field name="service_number" readonly="0" optional="1"/>
      </xpath>

    </field>
  </record>

</odoo>
