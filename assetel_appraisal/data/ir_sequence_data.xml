<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for apprasarial_period -->
        <record id="seq_appraisal_period" model="ir.sequence">
            <field name="name">AppraisalPeriod</field>
            <field name="code">appraisal.period</field>
            <field name="prefix">EVAL/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_appraisal_smart" model="ir.sequence">
            <field name="name">AppraisalSmart</field>
            <field name="code">appraisal.smart</field>
            <field name="prefix">SMART/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_appraisal_behavior" model="ir.sequence">
            <field name="name">AppraisalBehavior</field>
            <field name="code">appraisal.behavior</field>
            <field name="prefix">COMP/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>